#!/bin/csh -f

set xy = mapout.xy
set ps = mcheck.ps

set scale  = 30
set region = 139.4/139.6/34.0/34.2
set ba      = -Ba0.1f0.01/a0.1f0.01
set kp      = '-K -P'

psbasemap -R$region -Jx$scale -V $ba $kp -X1.5 -Y1.5 >! $ps
psxy $xy -R -Jx $kp -V -O -M -W0 >> $ps

psxy -R -Jx -V $kp -O -Sc0.08 -G255/0/0 -L -W1 <<END>> $ps
139.525000, 34.120278 
END

echo showpage >> $ps

gs $ps
#
